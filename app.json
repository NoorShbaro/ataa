{
  "expo": {
    "name": "Ataa",
    "slug": "Ataa",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "backgroundColor": "#070420",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/baner.png",
      "resizeMode": "contain",
      "backgroundColor": "#070420"
    },
    "newArchEnabled": true,
    "extra": {
      "supportsRTL": true,
      "forceRTL": false,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "40b8a4b6-6d27-4adb-b65c-3803ca023169"
      }
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "ExpoLocalization_forcesRTL": false,
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.ataafinance.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#070420"
      },
      "package": "com.ataafinance.app",
      "supportsRtl": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ATAAD.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#070420"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
